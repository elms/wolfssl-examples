           singleMappingRequired = "no"
      ></pool>
   </logicalMemoryPools>
   <usedFeatureSets>
      <usedFeatureSet
           name = "libansi"
           versionRequirement = "none"
           versionNumber = "none"
           featureSetUndefinedAction = "error"
