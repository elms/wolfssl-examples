           stackSizeInPages = "50"
           period = "fastest"
           namesOfMutexesLocked = ""
           usedToCreateISRThread = "no"
           schedulerName = "default-core-0"
      ></threadTemplate>
   </threadTemplates>
   <threadNames>
      <threadName
           name = "PrimaryThread"
           isMainThread = "yes"
           period = "fastest"
           schedulerName = "default-core-0"
      ></threadName>
